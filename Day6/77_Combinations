import java.util.ArrayList;
import java.util.List;

class Solution {
    public List<List<Integer>> combine(int n,int k){
        List<List<Integer>> ans=new ArrayList<>();
        List<Integer> current=new ArrayList<>();
        combinations(n,k,n,current,ans);
        return ans;
    }
    public void combinations(int n,int k,int currentNum,List<Integer> current,             List<List<Integer>> ans){
        if(k==0){
            ans.add(new ArrayList<>(current));
            return;
        }
        if(currentNum==0||k>currentNum) return;
        current.add(currentNum);
        combinations(n,k-1,currentNum-1,current,ans);
        current.remove(current.size()-1);
        combinations(n,k,currentNum-1,current,ans);
    }
}
